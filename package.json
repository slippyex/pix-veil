{
  "name": "@perengo/pix-veil",
  "version": "1.0.0",
  "main": "src/cli/index.ts",
  "config": {
    "permissions": "--unstable-kv --allow-env --allow-read --allow-ffi --allow-write --allow-sys"
  },
  "scripts": {
    "lint": "deno lint",
    "format": "deno fmt",
    "test": "deno test $npm_package_config_permissions",
    "doc": "deno doc --html --name='Pix-Veil' --output=./doc ./src",
    "coverage": "deno test --coverage $npm_package_config_permissions && deno coverage --html",
    "compile": "deno compile $npm_package_config_permissions --reload --output ./.bin/pix-veil  src/cli/index.ts",
    "pix-veil:encode": "deno run $npm_package_config_permissions src/cli/index.ts encode",
    "pix-veil:decode": "deno run $npm_package_config_permissionss src/cli/index.ts decode"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "chalk": "4",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "figlet": "^1.7.0",
    "inquirer": "^11.1.0",
    "lodash": "^4.17.21",
    "p-limit": "^6.1.0",
    "sharp": "^0.33.5",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/lodash": "^4.17.9",
    "@types/node": "^22.5.5",
    "@types/seedrandom": "^3.0.8",
    "seedrandom": "^3.0.5"
  }
}
